---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "chapter"  : "{{ post.chapter }}",
      "url"      : "{{ site.baseurl }}#page-{{ post.bookpage }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for page in site.pages %}
   {
     {% if page.title != nil %}
       "title"    : "{{ post.title | escape }}",
       "chapter"  : "{{ post.chapter }}",
       "url"      : "{{ site.baseurl }}#page-{{ post.bookpage }}",
       "date"     : "{{ post.date }}"
     {% endif %}
   } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
